<tr id="<%= dom_id friendspass %>">
  <td><%= link_to friendspass.first_name + ' ' + friendspass.last_name, friendspass %></td>
  <td><%= friendspass.email %></td>
  <td><%= friendspass.phone %></td>
  <td><%= friendspass.twitter %></td>
  <td><%= button_to "Delete", friendspass, method: :delete, data: {confirm: "Are you sure? "}, class:"btn btn-outline-danger btn-sm" %></td>
</tr>